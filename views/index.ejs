        <div class="div_positioning_dealer_card3">

        </div>

        <div class="div_positioning_dealer_card4">

        </div>

        <div class="div_positioning_dealer_card5">

        </div>

        <div class="div_positioning_dealer_card6">

        </div>

        <div class="div_positioning_dealer_card7">

        </div>

        <div class="div_positioning_dealer_card8">

        </div>


